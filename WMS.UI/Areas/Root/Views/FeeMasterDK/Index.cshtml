    function onkeyAddTen() {
        var fee = Math.ceil($("[name=OperationFee]").val());
        fee = fee + "";
        var feegw = fee.substr(fee.length - 1, 1);

        if (parseInt(feegw) != 0) {
            feegw = fee.substr(0, fee.length - 1) + "0";
            feegw = parseInt(feegw) + 10;
        }
        else { feegw = fee; }

        $("[name=OperationFee]").val(feegw);

        var fee = Number($("[name=OperationFee]").val()) - Number($("[name=hid_yushoufee]").val());
        if (Number(fee) == 0) {
            $("[name=feeChayi]").val(fee + " 元");
        }
        else if (Number(fee) < 0) {
            $("[name=feeChayi]").val("需退：" + fee + " 元");
        } else {
            $("[name=feeChayi]").val("还需收款：" + fee + " 元");
        }
    }

    function onkeyLoadAddTen() {
        var fee = Math.ceil($("[name=OperationFee_load]").val());

    //打印特操单
    function PrintReceiptId(obj) {
        var tr = eip.datagrid.getTr(obj)
        var row = eip.datagrid.getData(tr)
        var Id = row["系统编号"];
        var loadid = row["Load号"];

        if (loadid != null) {
            $.eipMsg.alert("请注意！该行TCR有Load号存在，本次需打印特操单及备货操作单！");
            eip.get("/OutBound/Load/PrintLoadId?LoadId=" + loadid, null, function (val) {
            window.open(val + "?load_id=" + Id);})
        }

        window.open("https://@Html.Raw(ViewBag.CrSaveAddress)/WMS_TCRFee.aspx?id=" + Id);
    }

    //打印出货操作单
    function PrintLoad(obj) {
